allow hal_vehicle_server hidl_base_hwservice:hwservice_manager add;
allow hal_vehicle_server su:tcp_socket { accept getopt read write };
allow hal_vehicle_server servicemanager:binder { call transfer };
allow hal_vehicle_server servicemanager:fd *;
allow hal_vehicle_client hal_vehicle_server:binder { call transfer };
allow hal_vehicle_client hal_vehicle_server:binder transfer;
allow hal_vehicle_client hal_vehicle_server:fd *;
allow hal_vehicle_client hal_vehicle_hwservice:hwservice_manager find;
allow hal_vehicle_client hal_vehicle_service:service_manager find;
allow hal_vehicle_default hwservicemanager:binder { call transfer };
allow hal_vehicle_default fwk_stats_hwservice:hwservice_manager find;
allow hal_vehicle_default stats_service_server:binder { call transfer };
allow hal_vehicle_default stats_service_server:fd *;
hal_server_domain(hal_vehicle_default, hal_vehicle)
hal_client_domain(hal_vehicle_default, hal_can_bus)
